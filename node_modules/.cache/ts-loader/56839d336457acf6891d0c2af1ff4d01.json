\n\nexport const store = createStore<State>({\n\n  strict: true,\n\n  state: {\n    count: 0,\n  },\n\n  mutations: {\n    increment(state) {\n      // mutate state\n      state.count += 1;\n    },\n  },\n\n});\n\n// define your own `useStore` composition function\nexport function useStore(): Store<State> {\n  return baseUseStore(key);\n}\n"],"sourceRoot":""}]}